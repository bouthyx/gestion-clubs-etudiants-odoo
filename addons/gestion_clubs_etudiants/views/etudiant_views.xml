<odoo>
    <record id="view_etudiant_tree" model="ir.ui.view">
        <field name="name">etudiant.tree</field>
        <field name="model">gestion.etudiant</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="email"/>
                <field name="club_id"/>
                <field name="date_adhesion"/>

            </tree>
        </field>
    </record>

    <record id="view_etudiant_form" model="ir.ui.view">
        <field name="name">etudiant.form</field>
        <field name="model">gestion.etudiant</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="email"/>
                        <field name="club_id"/>
                        <field name="date_adhesion"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
